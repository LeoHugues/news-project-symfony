{% trans_default_domain 'FOSUserBundle' %}

<form action="{{ path('fos_user_registration_register') }}" {{ form_enctype(form) }} method="POST" class="form-horizontal" novalidate="novalidate">
    {{ form_widget(form) }}
    <div>
        <div class="form-group">
            <div class="col-sm-offset-2 col-sm-10">
                <button type="submit" class="btn btn-success" value="{{ 'registration.submit'|trans }}">S'inscrire</button>
            </div>
        </div>
    </div>
</form>
